<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="res" type="ru.lb.fntestdojo.client.resources.AppResources"/>

    <ui:style>
        .content-panel{
            width: 450px;
            display: block;
            margin: 10px auto;
        }
    </ui:style>
    <g:VerticalPanel addStyleNames="{style.content-panel}">
        <g:SimplePanel ui:field="main"/>
    </g:VerticalPanel>
</ui:UiBinder>
