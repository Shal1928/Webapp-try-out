<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:VerticalPanel width="450px">
        <g:HTML>
            <h2>Тест № <span ui:field="testNumfield"/></h2><br/>
            <h3>Вопрос № <span ui:field="questionNumfield"/></h3><br/>
            <h4><span ui:field="questionTextfield"/></h4><br/>
        </g:HTML>
        <g:VerticalPanel ui:field="answersfield"/>
        <g:HTML><br/></g:HTML>
        <g:HorizontalPanel width="100%">
            <g:HorizontalPanel width="50%" ui:field="leftHorizontalfield"/>
            <g:HorizontalPanel width="50%" horizontalAlignment="ALIGN_RIGHT" ui:field="rightHorizontalfield"/>
        </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>
